<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:clipChildren="true"
	android:clipToPadding="true"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView
		android:paddingBottom="100dp"
		android:clipChildren="false"
		android:clipToPadding="false"
		android:id="@+id/rjzwucr9wkti"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="@drawable/cr40bffffff">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:clipChildren="false"
			android:clipToPadding="false"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/rjt9lufarub"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal"
				android:layout_marginTop="40dp"
				android:layout_marginBottom="50dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/rjjnqt5sw9vc"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center_horizontal"
					android:orientation="vertical">
					<TextView
						android:id="@+id/rtze9y5gof3j"
						android:textColor="#1D1517"
						android:textSize="20sp"
						android:textStyle="bold"
						android:layout_marginBottom="5dp"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="What is your goal ?"
					/>
					<TextView
						android:id="@+id/rrcb5be8gork"
						android:textColor="#7B6F72"
						android:textSize="12sp"
						android:layout_height="wrap_content"
						android:layout_width="177dp"
						android:gravity="center"
						android:text="It will help us to choose a best program for you"
					/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/r959m6c4gkxn"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginBottom="73dp"
				android:layout_marginStart="50dp"
				android:clipChildren="false"
				android:clipToPadding="false"
				android:orientation="horizontal">
				<RelativeLayout
					android:id="@+id/rg53yetc02fs"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="20dp"
					android:clipChildren="false"
					android:clipToPadding="false"
					>
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:clipChildren="false"
						android:clipToPadding="false"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/rom89h4lwpu"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal"
							android:background="@drawable/cr22lr495c050f6eea4ce"
							android:paddingVertical="35dp"
							android:paddingHorizontal="30dp"
							android:elevation="22dp"
							android:orientation="vertical">
							<com.google.android.material.imageview.ShapeableImageView
								android:id="@+id/rhelbn2nimp"
								android:layout_width="183dp"
								android:layout_height="290dp"
								android:layout_marginBottom="25dp"
								android:src="@mipmap/ic_launcher"
								android:scaleType="fitXY"
							/>
							<TextView
								android:id="@+id/rujea91vx3ua"
								android:textColor="#FFFFFF"
								android:textSize="14sp"
								android:textStyle="bold"
								android:layout_marginBottom="4dp"
								android:layout_height="wrap_content"
								android:layout_width="wrap_content"
								android:text="Improve Shape"
							/>
							<LinearLayout
								android:id="@+id/r1g6kp64thc8"
								android:layout_width="50dp"
								android:layout_height="1dp"
								android:background="#F7F8F8"
								android:layout_marginBottom="19dp"
								android:orientation="vertical">
							</LinearLayout>
							<TextView
								android:id="@+id/rwxbtk9c863"
								android:textColor="#FFFFFF"
								android:textSize="12sp"
								android:layout_height="wrap_content"
								android:layout_width="214dp"
								android:gravity="center"
								android:text="I have a low amount of body fat and need / want to build more muscle"
							/>
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/rv91hhxck6fq"
						android:layout_marginBottom="30dp"
						android:layout_marginStart="-50dp"
						android:layout_alignParentBottom="true"
						android:layout_width="205dp"
						android:layout_height="358dp"
						android:background="@drawable/cr22lr495c050f6eea4ce"
						android:elevation="22dp"
						android:clipChildren="false"
						android:clipToPadding="false"
						android:orientation="vertical">
					</LinearLayout>
				</RelativeLayout>
				<LinearLayout
					android:id="@+id/r1b2r4dnv6ti"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="end"
					android:background="@drawable/cr22lr495c050f6eea4ce"
					android:paddingTop="48dp"
					android:paddingBottom="30dp"
					android:layout_marginTop="90dp"
					android:elevation="22dp"
					android:clipChildren="false"
					android:clipToPadding="false"
					android:orientation="vertical">
					<com.google.android.material.imageview.ShapeableImageView
						android:id="@+id/r6693ogmqery"
						android:layout_width="1dp"
						android:layout_height="169dp"
						android:layout_marginBottom="23dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
					<TextView
						android:id="@+id/rmoczuvn5h9q"
						android:textColor="#B6B4C1"
						android:textSize="10sp"
						android:layout_marginTop="43dp"
						android:layout_marginBottom="1dp"
						android:layout_height="wrap_content"
						android:layout_width="5dp"
						android:gravity="center"
						android:text="I have over 20 lbs to lose. I want to drop all this fat and gain muscle mass"
					/>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
	<LinearLayout
		android:id="@+id/ry5dxvl1ozvi"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center_horizontal"
		android:background="@drawable/cr99lr495cc8fed6b50f6"
		android:paddingVertical="18dp"
		android:layout_marginBottom="40dp"
		android:layout_marginHorizontal="30dp"
		android:elevation="22dp"
		android:clipChildren="false"
		android:clipToPadding="false"
		android:clickable="true"
		android:focusable="true"
		android:orientation="vertical">
		<TextView
			android:id="@+id/rschrlrnu7t"
			android:textColor="#FFFFFF"
			android:textSize="16sp"
			android:textStyle="bold"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:text="Confirm"
			/>
	</LinearLayout>
</LinearLayout>
