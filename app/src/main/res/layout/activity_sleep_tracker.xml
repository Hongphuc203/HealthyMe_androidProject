<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:clipChildren="false"
	android:clipToPadding="false"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<ScrollView
		android:id="@+id/r9dxoka28sbp"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="@drawable/cr40bffffff"
		android:clipChildren="false"
		android:clipToPadding="false">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical"
			android:clipChildren="false"
			android:clipToPadding="false">

			<!-- Header “Sleep Tracker” -->
			<LinearLayout
				android:id="@+id/rr9j7tuvr9p"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal"
				android:gravity="center_vertical"
				android:layout_marginTop="40dp"
				android:layout_marginBottom="30dp"
				android:layout_marginHorizontal="30dp">

				<!-- Back button container -->
				<LinearLayout
					android:id="@+id/btnBackSleepTracker"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="90dp"
					android:background="@drawable/cr8bf7f8f8"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical"
					android:paddingHorizontal="13dp"
					android:paddingVertical="11dp">
					<ImageView
						android:id="@+id/rce5x3iprbul"
						android:layout_width="4dp"
						android:layout_height="9dp"
						android:scaleType="fitXY"
						android:src="@mipmap/ic_launcher" />
				</LinearLayout>

				<!-- Title “Sleep Tracker” -->
				<TextView
					android:id="@+id/rnl193sf2a9"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="90dp"
					android:layout_marginBottom="1dp"
					android:text="Sleep Tracker"
					android:textColor="#1D1517"
					android:textSize="16sp"
					android:textStyle="bold" />

				<!-- More options button -->
				<LinearLayout
					android:id="@+id/ru9os59brqco"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/cr8bf7f8f8"
					android:paddingVertical="14dp"
					android:paddingHorizontal="11dp"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical">
					<com.google.android.material.imageview.ShapeableImageView
						android:id="@+id/rnyrczzis2m"
						android:layout_width="10dp"
						android:layout_height="4dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY" />
				</LinearLayout>
			</LinearLayout>

			<!-- LineChart -->
			<com.github.mikephil.charting.charts.LineChart
				android:id="@+id/lineChart"
				android:layout_width="match_parent"
				android:layout_height="250dp"
				android:layout_marginVertical="30dp"
				android:layout_marginHorizontal="10dp"/>
			<!-- Last Night Sleep Card -->
			<LinearLayout
				android:id="@+id/r5sc0wwoleow"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="30dp"
				android:layout_marginBottom="30dp"
				android:background="@drawable/cr22lr49500ff6600efff"
				android:paddingTop="20dp"
				android:elevation="22dp"
				android:clipChildren="false"
				android:clipToPadding="false"
				android:orientation="vertical">

				<LinearLayout
					android:id="@+id/rrahc02g9p2c"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginStart="20dp"
					android:layout_marginBottom="3dp"
					android:orientation="vertical"
					android:clipChildren="false"
					android:clipToPadding="false">
					<TextView
						android:id="@+id/rv2ud35hbpy"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Last Night Sleep"
						android:textColor="#FFFFFF"
						android:textSize="14sp"
						android:textStyle="bold"
						android:layout_marginBottom="5dp"/>
					<TextView
						android:id="@+id/HoursOfSleepLastNight"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="8h 20m"
						android:textColor="#FFFFFF"
						android:textSize="14sp"
						android:textStyle="bold"/>
				</LinearLayout>

				<ImageView
					android:id="@+id/rk08uxx6kojg"
					android:layout_width="match_parent"
					android:layout_height="78dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"/>
			</LinearLayout>

			<!-- Daily Sleep Schedule -->

			<!-- Today Schedule Header -->
			<LinearLayout
				android:id="@+id/rb9u99gkwoe"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="30dp"
				android:layout_marginBottom="40dp"
				android:orientation="vertical"
				android:clipChildren="false"
				android:clipToPadding="false">
				<TextView
					android:id="@+id/res3h99z99je"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginBottom="16dp"
					android:text="Today Schedule"
					android:textColor="#1D1517"
					android:textSize="16sp"
					android:textStyle="bold"/>

				<!-- Bedtime Row -->
				<LinearLayout
					android:id="@+id/containerTodaySchedule"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="15dp"
					android:background="@drawable/cr20bffffff"
					android:orientation="horizontal"
					android:gravity="center_vertical"
					android:elevation="40dp"
					android:padding="10dp"
					android:clipChildren="false"
					android:clipToPadding="false">

					<ImageView
						android:id="@+id/rsumk43d3fe"
						android:layout_width="29dp"
						android:layout_height="29dp"
						android:layout_marginEnd="15dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"/>

					<LinearLayout
						android:id="@+id/rkzh9oplurbp"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:layout_marginEnd="12dp"
						android:orientation="vertical"
						android:clipChildren="false"
						android:clipToPadding="false">

						<TextView
							android:id="@+id/TimeToSleep"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginBottom="5dp"
							android:text="Bedtime, 09:00pm"
							android:textColor="#1D1517"
							android:textSize="14sp"
							android:textStyle="bold"/>

						<TextView
							android:id="@+id/TimeRemainingToSleep"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_weight="1"
							android:text="in 6hours 22minutes"
							android:textColor="#B6B4C1"
							android:textSize="14sp"/>
					</LinearLayout>

					<LinearLayout
						android:id="@+id/r4t9oo2m7c3c"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:gravity="end"
						android:clipChildren="false"
						android:clipToPadding="false">
						<ImageView
							android:id="@+id/rz2aquca7tfj"
							android:layout_width="14dp"
							android:layout_height="14dp"
							android:layout_marginBottom="20dp"
							android:src="@mipmap/ic_launcher"
							android:scaleType="fitXY"/>

						<com.google.android.material.switchmaterial.SwitchMaterial
							android:id="@+id/switchBedtime"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="0dp"
							android:showText="false"
							android:thumbTint="@color/white" />
					</LinearLayout>
				</LinearLayout>

				<!-- Alarm Row -->
				<LinearLayout
					android:id="@+id/r9fvvqcb6l88"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="horizontal"
					android:gravity="center_vertical"
					android:background="@drawable/cr20bffffff"
					android:elevation="40dp"
					android:clipChildren="false"
					android:clipToPadding="false"
					android:padding="10dp">

					<ImageView
						android:id="@+id/r83wjkwg0gqa"
						android:layout_width="27dp"
						android:layout_height="30dp"
						android:layout_marginEnd="16dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"/>

					<LinearLayout
						android:id="@+id/rk8rw1jz8efg"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:layout_marginEnd="12dp"
						android:orientation="vertical"
						android:clipChildren="false"
						android:clipToPadding="false">

						<TextView
							android:id="@+id/TimeToGetUp"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginBottom="5dp"
							android:text="Alarm, 05:10am"
							android:textColor="#1D1517"
							android:textSize="14sp"
							android:textStyle="bold"/>

						<TextView
							android:id="@+id/TimeRemainingGetUp"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_weight="1"
							android:text="in 14hours 30minutes"
							android:textColor="#B6B4C1"
							android:textSize="14sp"/>
					</LinearLayout>

					<LinearLayout
						android:id="@+id/r8wokcc7uhxg"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:gravity="end"
						android:clipChildren="false"
						android:clipToPadding="false">
						<ImageView
							android:id="@+id/re10l7j9kboj"
							android:layout_width="14dp"
							android:layout_height="14dp"
							android:layout_marginBottom="20dp"
							android:src="@mipmap/ic_launcher"
							android:scaleType="fitXY"/>
						<com.google.android.material.switchmaterial.SwitchMaterial
							android:id="@+id/switchAlarm"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:showText="false"
							android:thumbTint="@color/white"
							android:padding="0dp"/>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
