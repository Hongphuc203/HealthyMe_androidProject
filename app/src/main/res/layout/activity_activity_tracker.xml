<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView 
		android:id="@+id/rtab3wwz9m6d"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="@drawable/cr40bffffff">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/r4vg2m2b96uv"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginTop="40dp"
				android:layout_marginBottom="30dp"
				android:layout_marginHorizontal="30dp"
				android:orientation="horizontal">

				<LinearLayout
					android:id="@+id/btnBackActivityTracker"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="25dp"
					android:background="@drawable/cr8bf7f8f8"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical"
					android:paddingHorizontal="13dp"
					android:paddingVertical="11dp">

					<ImageView
						android:id="@+id/r9vxk0xpx8bh"
						android:layout_width="4dp"
						android:layout_height="9dp"
						android:scaleType="fitXY"
						android:src="@mipmap/ic_launcher" />
				</LinearLayout>

				<TextView
					android:id="@+id/AcitivityTracker"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_marginEnd="12dp"
					android:layout_marginBottom="1dp"
					android:layout_weight="1"
					android:gravity="center"
					android:text="Activity Tracker"
					android:textColor="#1D1517"
					android:textSize="16sp"
					android:textStyle="bold" />

				<LinearLayout
					android:id="@+id/rzltaurndrn"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/cr8bf7f8f8"
					android:paddingVertical="14dp"
					android:paddingHorizontal="11dp"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical">
					<com.google.android.material.imageview.ShapeableImageView
						android:id="@+id/rsvknvh3spf8"
						android:layout_width="10dp"
						android:layout_height="4dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY" />
				</LinearLayout>

			</LinearLayout>
			<LinearLayout
				android:id="@+id/TodayTarget"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/cr22lr495c050f6eea4ce"
				android:paddingVertical="20dp"
				android:layout_marginBottom="30dp"
				android:layout_marginHorizontal="30dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/r9a78w46ogzl"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:layout_marginBottom="15dp"
					android:layout_marginHorizontal="20dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/rvrb0j2fq7a"
						android:textColor="#1D1517"
						android:textSize="14sp"
						android:textStyle="bold"
						android:layout_marginBottom="1dp"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Today Target"
					/>
					<View
						android:layout_width="0dp"
						android:layout_height="0dp"
						android:layout_weight="1"
					 />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/ryt68kl85oul"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginHorizontal="20dp"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/WaterIntake"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:gravity="center_vertical"
						android:background="@drawable/cr12bffffff"
						android:paddingVertical="10dp"
						android:paddingHorizontal="9dp"
						android:layout_marginEnd="15dp"
						android:orientation="horizontal">
						<ImageView
							android:id="@+id/r8nibriua228"
							android:layout_width="25dp"
							android:layout_height="34dp"
							android:layout_marginEnd="8dp"
							android:src="@mipmap/ic_launcher"
							android:scaleType="fitXY"
						/>
						<LinearLayout
							android:id="@+id/r9b6kyzpuvzi"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:orientation="vertical">
							<TextView
								android:id="@+id/txtWaterIntake"
								android:textColor="#CC8FED"
								android:textSize="14sp"
								android:textStyle="bold"
								android:layout_height="wrap_content"
								android:layout_width="wrap_content"
								android:text="2000 ml"
							/>
							<TextView
								android:id="@+id/rwd0qjgasyec"
								android:textColor="#B6B4C1"
								android:textSize="12sp"
								android:layout_height="wrap_content"
								android:layout_width="wrap_content"
								android:text="Water Intake"
							/>
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/Footsteps"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:gravity="center_vertical"
						android:background="@drawable/cr12bffffff"
						android:paddingVertical="10dp"
						android:paddingLeft="8dp"
						android:orientation="horizontal">
						<ImageView
							android:id="@+id/ruxljn2fm4zq"
							android:layout_width="26dp"
							android:layout_height="25dp"
							android:layout_marginEnd="8dp"
							android:src="@mipmap/ic_launcher"
							android:scaleType="fitXY"
						/>
						<LinearLayout
							android:id="@+id/r16p12tuj6dv"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:orientation="vertical">
							<TextView
								android:id="@+id/txtFootStep"
								android:textColor="#CC8FED"
								android:textSize="14sp"
								android:textStyle="bold"
								android:layout_height="wrap_content"
								android:layout_width="wrap_content"
								android:text="8000"
							/>
							<TextView
								android:id="@+id/ri1vooe3xk4"
								android:textColor="#B6B4C1"
								android:textSize="12sp"
								android:layout_height="wrap_content"
								android:layout_width="wrap_content"
								android:text="Foot Steps"
							/>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>

				<TextView
					android:id="@+id/txtTargetMessage"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:textColor="#FFFFFF"
					android:textStyle="bold"
					android:textSize="14sp"
					android:layout_marginTop="16dp"
					android:layout_marginBottom="8dp"
					android:gravity="center" />

				<ProgressBar
					android:id="@+id/progressBarWater"
					style="@android:style/Widget.ProgressBar.Horizontal"
					android:layout_width="match_parent"
					android:layout_height="10dp"
					android:max="100"
					android:progressTint="#3E78F2"
					android:layout_marginHorizontal="20dp"
					android:layout_marginBottom="8dp" />

				<ProgressBar
					android:id="@+id/progressBarSteps"
					style="@android:style/Widget.ProgressBar.Horizontal"
					android:layout_width="match_parent"
					android:layout_height="10dp"
					android:max="100"
					android:progressTint="#009688"
					android:layout_marginHorizontal="20dp" />

			</LinearLayout>

			<LinearLayout
				android:id="@+id/rvr8z07et7zk"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="15dp"
				android:gravity="center_vertical"
				android:layout_marginHorizontal="30dp"
				android:orientation="horizontal">

				<TextView
					android:id="@+id/r625p9yhqr9c"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:text="Activity Progress"
					android:textColor="#1D1517"
					android:textSize="16sp"
					android:textStyle="bold" />

				<Spinner
					android:id="@+id/spinnerFilter"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:entries="@array/chart_filters"
					android:background="@drawable/cr50lr49500ff6600efff"
					android:paddingHorizontal="10dp"
					android:paddingVertical="6dp"
					android:textAlignment="center"
					android:textColor="#FFFFFF" />
			</LinearLayout>
					<com.github.mikephil.charting.charts.BarChart
						android:id="@+id/barChart"
						android:layout_width="match_parent"
						android:layout_height="200dp"
						android:paddingHorizontal="30dp"
						/>
			<LinearLayout
				android:id="@+id/rvbglvrwiex"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="40dp"
				android:layout_marginBottom="52dp"
				android:paddingHorizontal="30dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/rhfyvltyfpmj"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:layout_marginBottom="15dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/LatestActivity"
						android:textColor="#1D1517"
						android:textSize="16sp"
						android:textStyle="bold"
						android:layout_marginEnd="4dp"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:text="Latest Activity"
					/>
					<TextView
						android:id="@+id/txtSeeMore"
						android:textColor="#A5A3AF"
						android:textSize="12sp"
						android:textStyle="bold"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:gravity="right"
						android:text="See more"
					/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/containerLatestActivities"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					android:paddingTop="12dp"
					android:paddingBottom="12dp"/>

			</LinearLayout>

		</LinearLayout>
	</ScrollView>
	<LinearLayout
		android:id="@+id/btnAddActivity"
		android:layout_width="45dp"
		android:layout_height="47dp"
		android:layout_gravity="bottom|end"
		android:layout_margin="20dp"
		android:background="@drawable/cr8lr495cc8fed6b50f6"
		android:clickable="true"
		android:focusable="true"
		android:orientation="vertical"
		android:padding="5dp">

		<com.google.android.material.imageview.ShapeableImageView
			android:id="@+id/rs953glnnor"
			android:layout_width="35dp"
			android:layout_height="37dp"
			android:scaleType="fitXY"
			android:src="@mipmap/ic_launcher" />
	</LinearLayout>
</LinearLayout>
