				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="15dp"
				android:gravity="center_vertical"
				android:layout_marginHorizontal="30dp"
				android:orientation="horizontal">

				<TextView
					android:id="@+id/r625p9yhqr9c"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:text="Activity Progress"
					android:textColor="#1D1517"
					android:textSize="16sp"
					android:textStyle="bold" />

				<LinearLayout
					android:id="@+id/r3u6qoic76wf"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/cr50lr49500ff6600efff"
					android:clickable="true"
					android:focusable="true"
					android:gravity="center_vertical"
					android:orientation="horizontal"
					android:paddingHorizontal="10dp"
					android:paddingVertical="8dp">

					<TextView
						android:id="@+id/rf8spyrdng96"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginEnd="6dp"
						android:text="Weekly"
						android:textColor="#FFFFFF"
						android:textSize="10sp" />

					<ImageView
						android:id="@+id/rbqmyoczpn6k"
						android:layout_width="8dp"
						android:layout_height="4dp"
						android:layout_marginHorizontal="3dp"
						android:layout_marginVertical="5dp"
						android:scaleType="fitXY"
						android:src="@mipmap/ic_launcher" />
				</LinearLayout>
			</LinearLayout>
					<com.github.mikephil.charting.charts.BarChart
						android:id="@+id/barChart"
						android:layout_width="match_parent"
						android:layout_height="200dp"
						android:paddingHorizontal="30dp"
						/>
			<LinearLayout
				android:id="@+id/rvbglvrwiex"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="52dp"
				android:paddingHorizontal="30dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/rhfyvltyfpmj"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:layout_marginBottom="15dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/LatestActivity"
						android:textColor="#1D1517"
						android:textSize="16sp"
						android:textStyle="bold"
						android:layout_marginEnd="4dp"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:text="Latest Activity"
					/>
					<TextView
						android:id="@+id/r3oa8fmhiejm"
						android:textColor="#A5A3AF"
						android:textSize="12sp"
						android:textStyle="bold"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:gravity="right"
						android:text="See more"
					/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/rbsmp7rb8t3h"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="@drawable/cr16bffffff"
					android:paddingVertical="15dp"
					android:paddingHorizontal="21dp"
					android:layout_marginBottom="15dp"
					android:elevation="40dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/rockynw9luub"
						android:layout_width="35dp"
						android:layout_height="48dp"
						android:layout_marginTop="8dp"
						android:layout_marginEnd="16dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
					<LinearLayout
						android:id="@+id/r0fvqeqim5l8t"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:layout_marginTop="7dp"
						android:layout_marginEnd="12dp"
						android:orientation="vertical">
						<TextView
							android:id="@+id/r9z7azz4vath"
							android:textColor="#1D1517"
							android:textSize="12sp"
							android:textStyle="bold"
							android:layout_marginBottom="3dp"
							android:layout_height="wrap_content"
							android:layout_width="match_parent"
							android:layout_weight="1"
							android:text="Drinking 300ml Water"
						/>
						<TextView
							android:id="@+id/rmh5wqy46kt"
							android:textColor="#A3A8AC"
							android:textSize="10sp"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="About 3 minutes ago"
						/>
					</LinearLayout>
					<ImageView
						android:id="@+id/ryac7qjdxy7k"
						android:layout_width="14dp"
						android:layout_height="14dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/rllfu8f1k2fa"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="@drawable/cr16bffffff"
					android:padding="15dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/r4ay1hoewdwh"
						android:layout_width="39dp"
						android:layout_height="41dp"
						android:layout_marginTop="9dp"
						android:layout_marginStart="2dp"
						android:layout_marginEnd="10dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
					<LinearLayout
						android:id="@+id/r73y1lzivv56"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:gravity="center_horizontal"
						android:layout_marginTop="7dp"
						android:layout_marginEnd="12dp"
						android:orientation="vertical">
						<TextView
							android:id="@+id/rul7e6i8fi5"
							android:textColor="#1D1517"
							android:textSize="12sp"
							android:textStyle="bold"
							android:layout_marginBottom="3dp"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="Eat Snack (Fitbar)"
						/>
						<TextView
							android:id="@+id/r7yomowat2qj"
							android:textColor="#A3A8AC"
							android:textSize="10sp"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="About 10 minutes ago"
						/>
					</LinearLayout>
					<ImageView
						android:id="@+id/rc3wdu715gk"
						android:layout_width="14dp"
						android:layout_height="14dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
