<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView
		android:id="@+id/rhwxo7rttwzs"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:gravity="center_horizontal"
		android:background="@drawable/cr30bffffff"
		android:paddingHorizontal="30dp">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">

			<LinearLayout
				android:id="@+id/rh8wxjy883sj"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="40dp"
				android:layout_marginBottom="30dp"
				android:gravity="center_horizontal"
				android:orientation="vertical">

				<TextView
					android:id="@+id/rgdamvmq5j3"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginBottom="5dp"
					android:text="Hey there,"
					android:textColor="#1D1517"
					android:textSize="16sp" />

				<TextView
					android:id="@+id/rznawfi0eruk"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Welcome Back"
					android:textColor="#1D1517"
					android:textSize="20sp"
					android:textStyle="bold" />
			</LinearLayout>

			<LinearLayout
				android:id="@+id/r0rbo1t1xefh"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal"
				android:layout_marginBottom="285dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/r7xcaf69jl7a"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:background="@drawable/sf7f8f8sw1cr14bf7f8f8"
					android:paddingVertical="15dp"
					android:paddingLeft="15dp"
					android:paddingRight="3dp"
					android:layout_marginBottom="15dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/rm9gx8tna7qr"
						android:layout_width="15dp"
						android:layout_height="13dp"
						android:layout_marginVertical="2dp"
						android:layout_marginStart="1dp"
						android:layout_marginEnd="11dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
						/>
					<EditText
						android:inputType="text"
						android:hint="Email"
						android:background="@null"
						android:id="@+id/ryujd5w5degp"
						android:textColor="#ACA3A5"
						android:textSize="12sp"
						android:layout_width="0dp"
						android:layout_height="30dp"
						android:layout_weight="1"
						/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/riyto5hxdpz"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="@drawable/sf7f8f8sw1cr14bf7f8f8"
					android:padding="15dp"
					android:layout_marginBottom="10dp"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/raic3fny61q7"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingRight="3dp"
						android:orientation="horizontal">
						<ImageView
							android:id="@+id/ridf6rzh4fx"
							android:layout_width="11dp"
							android:layout_height="13dp"
							android:layout_marginVertical="2dp"
							android:layout_marginStart="3dp"
							android:layout_marginEnd="13dp"
							android:src="@mipmap/ic_launcher"
							android:scaleType="fitXY"
							/>
						<TextView
							android:id="@+id/rnn9ey021lyb"
							android:textColor="#ACA3A5"
							android:textSize="12sp"
							android:layout_height="30dp"
							android:layout_width="wrap_content"
							android:text="Password"
							/>
					</LinearLayout>
					<View
						android:layout_width="0dp"
						android:layout_height="0dp"
						android:layout_weight="1"
						/>
					<ImageView
						android:id="@+id/rf7w61n68etu"
						android:layout_width="13dp"
						android:layout_height="11dp"
						android:layout_marginVertical="3dp"
						android:layout_marginHorizontal="2dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
						/>
				</LinearLayout>
				<TextView
					android:id="@+id/rqa66oztsoqh"
					android:textColor="#ACA3A5"
					android:textSize="12sp"
					android:textStyle="bold"
					android:layout_marginBottom="1dp"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Forgot your password?"
					/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/rriou8zz9xb"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal"
				android:background="@drawable/cr99lr495cc8fed6b50f6"
				android:paddingVertical="18dp"
				android:layout_marginBottom="20dp"
				android:clickable="true"
				android:focusable="true"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/r9epkkr5jvwq"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingRight="3dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/ri63vs72r6vl"
						android:layout_width="19dp"
						android:layout_height="20dp"
						android:layout_marginVertical="2dp"
						android:layout_marginStart="2dp"
						android:layout_marginEnd="12dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
						/>
					<TextView
						android:id="@+id/rn6eyewu9i5j"
						android:textColor="#FFFFFF"
						android:textSize="16sp"
						android:textStyle="bold"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Login"
						/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/rd6bcm3xexl9"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginBottom="20dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/rdpbxnncp84m"
					android:layout_width="141dp"
					android:layout_height="1dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
					/>
				<View
					android:layout_width="0dp"
					android:layout_height="0dp"
					android:layout_weight="1"
					/>
				<TextView
					android:id="@+id/rk7i3pcxy5t"
					android:textColor="#1D1517"
					android:textSize="12sp"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Or"
					/>
				<View
					android:layout_width="0dp"
					android:layout_height="0dp"
					android:layout_weight="1"
					/>
				<ImageView
					android:id="@+id/rlsujbrsp6y"
					android:layout_width="140dp"
					android:layout_height="1dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
					/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/r9zp3bs988m8"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginBottom="30dp"
				android:orientation="horizontal">

				<LinearLayout
					android:id="@+id/rwt2nem0tk7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="30dp"
					android:background="@drawable/sddd9dasw1cr14"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical"
					android:padding="15dp"
					tools:ignore="SpeakableTextPresentCheck">

					<com.google.android.material.imageview.ShapeableImageView
						android:id="@+id/r82m4wguhbh"
						android:layout_width="20dp"
						android:layout_height="20dp"
						android:scaleType="fitXY"
						android:src="@mipmap/ic_launcher" />
				</LinearLayout>

				<LinearLayout
					android:id="@+id/rmfd4wcx9iu"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/sddd9dasw1cr14"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical"
					android:padding="15dp"
					tools:ignore="SpeakableTextPresentCheck">

					<com.google.android.material.imageview.ShapeableImageView
						android:id="@+id/r0093rj4wtn8zb"
						android:layout_width="20dp"
						android:layout_height="20dp"
						android:scaleType="fitXY"
						android:src="@mipmap/ic_launcher" />
				</LinearLayout>
			</LinearLayout>
			<TextView
				android:id="@+id/ra4o77up0cft"
				android:textColor="#1D1517"
				android:textSize="14sp"
				android:layout_marginBottom="41dp"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:text="Donâ€™t have an account yet? Register"
				/>
		</LinearLayout>
	</ScrollView>
</LinearLayout>