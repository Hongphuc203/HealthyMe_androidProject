<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView 
		android:id="@+id/rh0cn1gwzlfp"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="@drawable/cr40bffffff">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/rb4fllddexm5"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginTop="40dp"
				android:layout_marginBottom="30dp"
				android:layout_marginHorizontal="30dp"
				android:orientation="horizontal">

				<LinearLayout
					android:id="@+id/rnunfnuq0nfb"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="25dp"
					android:background="@drawable/cr8bf7f8f8"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical"
					android:paddingHorizontal="13dp"
					android:paddingVertical="11dp">

					<ImageView
						android:id="@+id/rqvsr6hwwyom"
						android:layout_width="4dp"
						android:layout_height="9dp"
						android:scaleType="fitXY"
						android:src="@mipmap/ic_launcher" />
				</LinearLayout>
				<TextView
					android:id="@+id/AddAlarm"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_marginEnd="12dp"
					android:layout_marginBottom="1dp"
					android:layout_weight="1"
					android:gravity="center"
					android:text="Activity Tracker"
					android:textColor="#1D1517"
					android:textSize="16sp"
					android:textStyle="bold" />

				<LinearLayout
					android:id="@+id/rfg24yacflm4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/cr8bf7f8f8"
					android:paddingVertical="14dp"
					android:paddingHorizontal="11dp"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical">
					<com.google.android.material.imageview.ShapeableImageView
						android:id="@+id/rux5dw8xkxfp"
						android:layout_width="10dp"
						android:layout_height="4dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/Bedtime"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:background="@drawable/cr16bf7f8f8"
				android:padding="15dp"
				android:layout_marginBottom="10dp"
				android:layout_marginHorizontal="30dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/rlvxxdkwfal9"
					android:layout_width="20dp"
					android:layout_height="20dp"
					android:layout_marginEnd="10dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
				/>
				<LinearLayout
					android:id="@+id/r3jx97qo41zy"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:gravity="center_vertical"
					android:layout_marginEnd="5dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/rqi1s3416zu"
						android:textColor="#B6B4C1"
						android:textSize="12sp"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:text="Bedtime"
					/>
					<TextView
						android:id="@+id/r3le22x9f6s"
						android:textColor="#A5A3AF"
						android:textSize="10sp"
						android:layout_marginEnd="3dp"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="09:00 PM"
					/>
				</LinearLayout>
				<ImageView
					android:id="@+id/r0rf45fm8gc59"
					android:layout_width="5dp"
					android:layout_height="11dp"
					android:layout_marginVertical="4dp"
					android:layout_marginHorizontal="7dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
				/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/HoursSleep"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:background="@drawable/cr16bf7f8f8"
				android:padding="15dp"
				android:layout_marginBottom="10dp"
				android:layout_marginHorizontal="30dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/rmg2zexi1jf"
					android:layout_width="15dp"
					android:layout_height="15dp"
					android:layout_marginVertical="2dp"
					android:layout_marginStart="2dp"
					android:layout_marginEnd="12dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
				/>
				<LinearLayout
					android:id="@+id/rrxagueqtnz"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:gravity="center_vertical"
					android:layout_marginEnd="5dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/r74mrad24wcb"
						android:textColor="#B6B4C1"
						android:textSize="12sp"
						android:layout_marginEnd="57dp"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Hours of sleep"
					/>
					<TextView
						android:id="@+id/rdlzwk4xubh"
						android:textColor="#A5A3AF"
						android:textSize="10sp"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:gravity="right"
						android:text="8hours 30minutes"
					/>
				</LinearLayout>
				<ImageView
					android:id="@+id/r88txiux8z89"
					android:layout_width="5dp"
					android:layout_height="11dp"
					android:layout_marginVertical="4dp"
					android:layout_marginHorizontal="7dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
				/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/Repeat"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:background="@drawable/cr16bf7f8f8"
				android:padding="15dp"
				android:layout_marginBottom="10dp"
				android:layout_marginHorizontal="30dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/ri2qk5xn607"
					android:layout_width="19dp"
					android:layout_height="16dp"
					android:layout_marginVertical="1dp"
					android:layout_marginEnd="10dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
				/>
				<LinearLayout
					android:id="@+id/rmezpw0529im"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:gravity="center_vertical"
					android:layout_marginEnd="5dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/rc8khqbuu01m"
						android:textColor="#B6B4C1"
						android:textSize="12sp"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:text="Repeat"
					/>
					<TextView
						android:id="@+id/r1qkg0dqa4pa"
						android:textColor="#A5A3AF"
						android:textSize="10sp"
						android:layout_marginEnd="3dp"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Mon to Fri"
					/>
				</LinearLayout>
				<ImageView
					android:id="@+id/rh4m7qhswoz9"
					android:layout_width="5dp"
					android:layout_height="11dp"
					android:layout_marginVertical="4dp"
					android:layout_marginHorizontal="7dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
				/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/Vibrate"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:background="@drawable/cr16bf7f8f8"
				android:paddingVertical="13dp"
				android:paddingHorizontal="15dp"
				android:layout_marginBottom="380dp"
				android:layout_marginHorizontal="30dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/rla5cm6tja5"
					android:layout_width="19dp"
					android:layout_height="20dp"
					android:layout_marginEnd="10dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
				/>
				<EditText
					android:inputType="text"
					android:hint="vibrate When Alarm Sound"
					android:background="@null"
					android:id="@+id/rvsw7gvo5r9i"
					android:textColor="#B6B4C1"
					android:textSize="12sp"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:paddingVertical="3dp"
				/>
				<LinearLayout
					android:id="@+id/rx1fdzqu74l"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/cr50lr49500ff6600efff"
					android:paddingVertical="5dp"
					android:paddingLeft="25dp"
					android:paddingRight="5dp"
					android:orientation="vertical">
					<com.google.android.material.imageview.ShapeableImageView
						android:id="@+id/r3faa1kretc9"
						android:layout_width="14dp"
						android:layout_height="14dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/AddButton"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal"
				android:background="@drawable/cr99lr495cc8fed6b50f6"
				android:paddingVertical="18dp"
				android:layout_marginBottom="40dp"
				android:layout_marginHorizontal="31dp"
				android:elevation="22dp"
				android:clickable="true"
				android:focusable="true"
				android:orientation="vertical">
				<TextView
					android:id="@+id/r0oxzizv1i1gf"
					android:textColor="#FFFFFF"
					android:textSize="16sp"
					android:textStyle="bold"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Add"
				/>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
