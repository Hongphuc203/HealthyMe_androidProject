<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/rwu5n3c172c"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginTop="40dp"
				android:layout_marginBottom="30dp"
				android:layout_marginHorizontal="30dp"
				android:orientation="horizontal">

				<LinearLayout
					android:id="@+id/rvccip4dzft9"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="25dp"
					android:background="@drawable/cr8bf7f8f8"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical"
					android:padding="8dp">

					<com.google.android.material.imageview.ShapeableImageView
						android:id="@+id/rxzpwwu4gzna"
						android:layout_width="16dp"
						android:layout_height="16dp"
						android:scaleType="fitXY"
						android:src="@mipmap/ic_launcher" />
				</LinearLayout>
				<TextView
					android:id="@+id/AddSchedule"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_marginEnd="12dp"
					android:layout_marginBottom="1dp"
					android:layout_weight="1"
					android:gravity="center"
					android:text="Activity Tracker"
					android:textColor="#1D1517"
					android:textSize="16sp"
					android:textStyle="bold" />
				<LinearLayout
					android:id="@+id/rpmpnzv6d39"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/cr8bf7f8f8"
					android:paddingVertical="14dp"
					android:paddingHorizontal="11dp"
					android:clickable="true"
					android:focusable="true"
					android:orientation="vertical">
					<com.google.android.material.imageview.ShapeableImageView
						android:id="@+id/rliefy2nfuja"
						android:layout_width="10dp"
						android:layout_height="4dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/r9cjpd0cl7zq"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginBottom="30dp"
				android:layout_marginStart="30dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/rhc3u23lj0xa"
					android:layout_width="16dp"
					android:layout_height="18dp"
					android:layout_marginVertical="1dp"
					android:layout_marginStart="2dp"
					android:layout_marginEnd="12dp"
					android:src="@mipmap/ic_launcher"
					android:scaleType="fitXY"
				/>
				<TextView
					android:id="@+id/DateNow"
					android:textColor="#B6B4C1"
					android:textSize="14sp"
					android:layout_marginBottom="1dp"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Thu, 27 May 2022"
				/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/rkin30o2nnm8"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="30dp"
				android:orientation="vertical">
				<TextView
					android:id="@+id/rpmzueca3lf"
					android:textColor="#1D1517"
					android:textSize="14sp"
					android:textStyle="bold"
					android:layout_marginBottom="10dp"
					android:layout_marginStart="30dp"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Time"
				/>
				<TimePicker
					android:id="@+id/timePicker"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:timePickerMode="spinner"
					android:layout_marginHorizontal="30dp"
					android:layout_marginBottom="30dp"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/rnkucte6hkhl"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="159dp"
				android:layout_marginHorizontal="30dp"
				android:orientation="vertical">
				<TextView
					android:id="@+id/r1mh7ac5rc4y"
					android:textColor="#1D1517"
					android:textSize="14sp"
					android:textStyle="bold"
					android:layout_marginBottom="10dp"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Details Workout"
				/>
				<LinearLayout
					android:id="@+id/ChooseWorkOut"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:background="@drawable/cr16bf7f8f8"
					android:padding="15dp"
					android:layout_marginBottom="10dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/r3imphkoshc1"
						android:layout_width="20dp"
						android:layout_height="20dp"
						android:layout_marginEnd="10dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
					<LinearLayout
						android:id="@+id/ruvpanxjk21n"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:gravity="center_vertical"
						android:layout_marginEnd="5dp"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/TextHiddenChoose"
							android:textColor="#B6B4C1"
							android:textSize="12sp"
							android:layout_marginEnd="32dp"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="Choose Workout"
						/>
						<TextView
							android:id="@+id/ralt3t33spr9"
							android:textColor="#A5A3AF"
							android:textSize="10sp"
							android:layout_height="wrap_content"
							android:layout_width="match_parent"
							android:layout_weight="1"
							android:text="Upperbody Workout"
						/>
					</LinearLayout>
					<ImageView
						android:id="@+id/r464rjbxczs4"
						android:layout_width="5dp"
						android:layout_height="11dp"
						android:layout_marginVertical="4dp"
						android:layout_marginHorizontal="7dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/Difficult"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:background="@drawable/cr16bf7f8f8"
					android:padding="15dp"
					android:layout_marginBottom="10dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/rguwaci3ae6g"
						android:layout_width="15dp"
						android:layout_height="13dp"
						android:layout_marginVertical="3dp"
						android:layout_marginStart="2dp"
						android:layout_marginEnd="12dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
					<LinearLayout
						android:id="@+id/r54a2h34tpur"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight="1"
						android:gravity="center_vertical"
						android:layout_marginEnd="5dp"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/TextHiddenDiff"
							android:textColor="#B6B4C1"
							android:textSize="12sp"
							android:layout_marginEnd="32dp"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="Choose Workout"
							/>

						<TextView
							android:id="@+id/r57cclhpmaum"
							android:textColor="#A5A3AF"
							android:textSize="10sp"
							android:layout_marginEnd="2dp"
							android:layout_height="wrap_content"
							android:layout_width="wrap_content"
							android:text="Beginner"
						/>
					</LinearLayout>
					<ImageView
						android:id="@+id/rkdyy1kyhaq9"
						android:layout_width="5dp"
						android:layout_height="11dp"
						android:layout_marginVertical="4dp"
						android:layout_marginHorizontal="7dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/Repetition"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:background="@drawable/cr16bf7f8f8"
					android:padding="15dp"
					android:layout_marginBottom="10dp"
					android:clickable="true"
					android:focusable="true"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/rb5d42e9ya7u"
						android:layout_width="16dp"
						android:layout_height="16dp"
						android:layout_marginVertical="1dp"
						android:layout_marginStart="1dp"
						android:layout_marginEnd="11dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>

					<TextView
						android:id="@+id/r06p81pd51yux"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginEnd="165dp"
						android:layout_marginBottom="1dp"
						android:text="Custom Repetitions"
						android:textColor="#B6B4C1"
						android:textSize="12sp" />
					<ImageView
						android:id="@+id/r4bkzgixg1lp"
						android:layout_width="5dp"
						android:layout_height="11dp"
						android:layout_marginVertical="4dp"
						android:layout_marginHorizontal="7dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/rv5lc4ki1ty9"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal"
				android:background="@drawable/cr99lr495cc8fed6b50f6"
				android:paddingVertical="18dp"
				android:layout_marginBottom="40dp"
				android:layout_marginHorizontal="30dp"
				android:elevation="22dp"
				android:clickable="true"
				android:focusable="true"
				android:orientation="vertical">
				<TextView
					android:id="@+id/ridjyzc4gh2c"
					android:textColor="#FFFFFF"
					android:textSize="16sp"
					android:textStyle="bold"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Save"
				/>
			</LinearLayout>
		</LinearLayout>
</LinearLayout>
